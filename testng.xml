<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="parallel Test Suite" parallel="classes">

	<test name="Chrome Test">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="88.0" />
		<parameter name="platform" value="Windows 10" />

		<classes>
			<class name="TestCasePackage.SeleniumTestNgTestCase" />
		</classes>
	</test> <!-- Test -->

	<test name="Edge Test">
		<parameter name="browser" value="Edge" />
		<parameter name="version" value="87.0" />
		<parameter name="platform" value="macOS Sierra" />

		<classes>
			<class name="TestCasePackage.SeleniumTestNgTestCase" />
		</classes>
	</test> <!-- Test -->

	<test name="FF Test">
		<parameter name="browser" value="Firefox" />
		<parameter name="version" value="82.0" />
		<parameter name="platform" value="Windows 7" />

		<classes>
			<class name="TestCasePackage.SeleniumTestNgTestCase" />
		</classes>
	</test> <!-- Test -->

	<test name="IE Test">
		<parameter name="browser" value="IE" />
		<parameter name="version" value="11.0" />
		<parameter name="platform" value="Windows 10" />

		<classes>
			<class name="TestCasePackage.SeleniumTestNgTestCase" />
		</classes>
	</test> <!-- Test -->





</suite> <!-- Suite -->
